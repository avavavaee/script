local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Robojini/Tuturial_UI_Library/main/UI_Template_1"))()
local Window = Library.CreateLib("Femboy hub", "RJTheme3")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main")

local kil = nil
local teleporting = false

local function startTeleportation()
    -- Поиск объекта "Kil" в Workspace
    kil = game.Workspace:FindFirstChild("Kil")
    
    -- Если объект не найден, выводим предупреждение и останавливаем выполнение
    if not kil then
        warn("Объект с именем 'Kil' не найден в Workspace!")
        return
    end

    -- Переносим объект далеко за карту
    kil.CFrame = CFrame.new(0, 100000, 0)

    -- Устанавливаем флаг teleporting в true
    teleporting = true

    -- Запускаем цикл телепортации
    while teleporting do
        wait(15) -- Ждем 15 секунд

        -- Проверяем, существует ли объект "Kil" и доступен ли игрок
        if kil and kil.Parent and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = kil.CFrame
        else
            warn("Объект 'Kil' или игрок не найдены. Остановка телепортации.")
            teleporting = false
            break
        end
    end
end

local function stopTeleportation()
    teleporting = false
    print("Телепортация остановлена.")
end

-- Создаем переключатель для автофарма
Section:NewToggle("autofarm", "авто фарм", function(state)
    if state then
        startTeleportation()
    else
        stopTeleportation()
    end
end)
