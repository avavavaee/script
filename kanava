local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Robojini/Tuturial_UI_Library/main/UI_Template_1"))()

local Window = Library.CreateLib("Femboy hub", "RJTheme3")

local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Main")

local kil = nil
local teleporting = false

local function startTeleportation()
    kil = game.Workspace:FindFirstChild("Kil")
    
    if not kil then
        warn("Объект с именем 'Kil' не найден в Workspace!")
        return
    end

    kil.CFrame = CFrame.new(0, 100000, 0)
    teleporting = true

    while teleporting do
        wait(15)
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = kil.CFrame
        else
            warn("Character or HumanoidRootPart not found!")
            break
        end
    end
end

local function stopTeleportation()
    teleporting = false
    print("Телепортация остановлена.")
end

Section:NewToggle("autofarm", "авто фарм", function(state)
    if state then
        startTeleportation()
    else
        stopTeleportation()
    end
end)

local isRunning = false

local function hasHandle(folder)
    for _, item in pairs(folder:GetChildren()) do
        if item.Name == "Handle" then
            return true
        end
    end
    return false
end

local function mainLoop()
    while isRunning do
        local player = game.Players.LocalPlayer

        if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            for _, folder in pairs(game.Workspace:GetChildren()) do
                if folder:IsA("Folder") and hasHandle(folder) then
                    for _, item in pairs(folder:GetChildren()) do
                        if item:IsA("BasePart") then
                            item.CFrame = player.Character.HumanoidRootPart.CFrame
                        end
                    end
                end
            end
        else
            warn("Character or HumanoidRootPart not found!")
            break
        end

        wait(3)
    end
end

local function vkl()
    if not isRunning then
        isRunning = true
        mainLoop()
    end
end

local function otkl()
    isRunning = false
end

Section:NewToggle("bring all tools", "притягивает все оружия", function(state)
    if state then
        vkl()
    else
        otkl()
    end
end)

Section:NewButton("guffy walk (test)", "эффект от мефа", function()
local plr = game.Players.LocalPlayer
local char = plr.Character
local Hum = char.Humanoid

while wait() do
    Hum:ChangeState("FallingDown")
    wait(.15)
    Hum:ChangeState("GettingUp")
    Hum:ChangeState("RunningNoPhysics")
end
end)

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local PVPToggle = ReplicatedStorage.Remotes.PVPToggle


local isLoopActive = false


local function enablePVP()
    local args = { [1] = true }
    PVPToggle:InvokeServer(unpack(args))
    print("PVP включен")
end


local function disablePVP()
    local args = { [1] = false }
    PVPToggle:InvokeServer(unpack(args))
    print("PVP выключен")
end

local function togglePVPLoop(interval)
    while isLoopActive do
        enablePVP()
        wait(interval) 
        disablePVP()
        wait(interval) 
    end
end


local function startLoop(interval)
    if not isLoopActive then
        isLoopActive = true
        togglePVPLoop(interval)
    end
end


local function stopLoop()
    if isLoopActive then
        isLoopActive = false
    end
end


local interval = 1 





Section:NewToggle("spam pvp", "спамит пвп", function(state)
    if state then
startLoop()
    else
stopLoop()
    end
end)
Section:NewButton("red pvp (test)", "делает красную надпись пвп", function()

local args = {
    [1] = true
}

for i = 1, 50 do
    game:GetService("ReplicatedStorage").Remotes.PVPToggle:InvokeServer(unpack(args))
    wait(0.1)  -- Небольшая задержка между вызовами, чтобы избежать возможных проблем
end

end)


Section:NewTextBox("tablet text (create it first)", "пишет на табличке все что угодно", function(txt)
    -- Функция для обработки текста по две буквы
    local function processText(text)
        for i = 1, #text, 2 do
            local twoLetters = text:sub(i, i+1)  -- Берем по две буквы
            -- Если буква одна, добавляем пробел (или любой другой символ)
            if #twoLetters < 2 then
                twoLetters = twoLetters .. " "  -- Добавляем пробел для второй буквы
            end
            
            local args = {
                [1] = "text",  -- Первый аргумент остается "text"
                [2] = twoLetters  -- Второй аргумент — это две буквы
            }
            
            -- Вызываем функцию с текущими двумя буквами
            game:GetService("ReplicatedStorage").Remotes.SignManager:InvokeServer(unpack(args))
        end
    end
    
    -- Вызываем функцию для обработки текста
    processText(txt)
end)






local Tab = Window:NewTab("Teleports")
local Section = Tab:NewSection("Teleports")

Section:NewButton("roof", "телепортирует на крышу", function()
    if game.Players.LocalPlayer.Character then
        game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(-230.5, 2105.0498046875, 213)))
    end
end)

Section:NewButton("graveyard", "телепортирует на кладбище", function()
    if game.Players.LocalPlayer.Character then
        game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(-120.25, 2050.2998046875, -110)))
    end
end)

Section:NewButton("sewage system", "телепортирует в канализацию", function()
    if game.Players.LocalPlayer.Character then
        game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(-224, 2036, -82.25)))
    end
end)

Section:NewButton("tower", "телепортирует на башню", function()
    if game.Players.LocalPlayer.Character then
        game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(114.60832977294922, 2110.345703125, 218.41067504882812)))
    end
end)

Section:NewDropdown("Tables", "DropdownInf", {"Table 1", "Table 2", "Table 3"}, function(tables)
    if tables == "Table 1" then
        game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(150.5, 2060.0498046875, 136)))
    elseif tables == "Table 2" then
        game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(-168.5, 2060.0498046875, 192))) 
    elseif tables == "Table 3" then
        game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(-230.5, 2105.0498046875, 213)))
    end
end)


Section:NewButton("spawn", "телепортирует на спавн", function()
    if game.Players.LocalPlayer.Character then
        game.Players.LocalPlayer.Character:PivotTo(CFrame.new(Vector3.new(27.429304122924805, 2052.999267578125, -30.69707679748535)))
    end
end)









local Tab = Window:NewTab("Misc")


local Section = Tab:NewSection("Misc")


Section:NewSlider("Time", "Меняет время суток", 24, 0, function(s) 

local Lighting = game:GetService("Lighting")

Lighting.ClockTime = s

end)

local Noclip = nil
local Clip = nil

function noclip()
    Clip = false
    local function Nocl()
        if Clip == false and game.Players.LocalPlayer.Character ~= nil then
            for _,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if v:IsA('BasePart') and v.CanCollide and v.Name ~= floatName then
                    v.CanCollide = false
                end
            end
        end
        wait(0.21)
    end
    Noclip = game:GetService('RunService').Stepped:Connect(Nocl)
end

function clip()
    if Noclip then Noclip:Disconnect() end
    Clip = true
end


Section:NewToggle("Noclip", "Позволяет проходить сквозь стены", function(state)
    if state then
noclip()
    else
clip()
    end
end)

Section:NewSlider("Speed", "Изменяет скорость", 500, 0, function(g)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = g
end)

Section:NewSlider("Gravity", "Изменяет гравитацию", 500, 0, function(h)
    game.Workspace.Gravity = h
end)

Section:NewSlider("JumpPower", "Изменяет силу прыжка", 500, 0, function(j)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = j
end)

Section:NewToggle("Off gravity", "Отключает гравитацию", function(state)
    if state then
game.Workspace.Gravity = true
    else
game.Workspace.Gravity = 196.1999969482422
    end
end)


Section:NewToggle("Superhuman", "Меняет скорость и силу прыжка", function(state)
    if state then
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 110
    else

game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)







